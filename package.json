{
  "name": "@atxm/open-package-settings",
  "version": "0.0.0",
  "description": "Opens package settings for the Atom package that consumes this module",
  "license": "MIT OR GPL-2.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "npm run start",
    "fix": "eslint --fix ./src",
    "lint": "eslint ./src",
    "start": "rollup -w -c"
  },
  "main": "./lib/index.js",
  "typings": "./types",
  "type": "module",
  "homepage": "https://www.npmjs.com/package/@atxm/open-package-settings",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-t-x-m/open-package-settings.git"
  },
  "keywords": [
    "atom",
    "atom package",
    "package settings",
    "settings"
  ],
  "dependencies": {
    "@atxm/developer-console": "^0.2.2",
    "@atxm/get-package-name": "^0.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/atom": "^1.40.4",
    "@types/node": "^14.6.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "husky": "^4.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.8",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
